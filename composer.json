{
  "name": "aurion72/helpers",
  "description": "Aurion72 Helpers",
  "license": "MIT",
  "keywords": [
    "helpers"
  ],
  "type": "library",
  "authors": [
    {
      "name": "Anthony Aslangul",
      "email": "anthony.aslangul@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.1.3",
    "illuminate/support": "~5.6|^6.0",
    "bkwld/croppa": "^4.7"
  },
  "require-dev": {
    "orchestra/testbench": "~3.6.0",
    "phpunit/phpunit": "~7.0"
  },
  "autoload": {
    "psr-4": {
      "Aurion72\\Helpers\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Aurion72\\Helpers\\Tests\\": "tests"
    },
    "files": [
      "vendor/phpunit/phpunit/src/Framework/Assert/Functions.php"
    ]
  },
  "scripts": {
    "phpunit": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Aurion72\\Helpers\\HelpersServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
